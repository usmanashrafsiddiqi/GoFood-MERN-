{
    "version": 2,
    "builds": [
      {
        "src": "src/package.json",  
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"  
        }
      },
      {
        "src": "backend/server.js",  
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",  
        "dest": "/backend/$1"
      },
      {
        "src": "/(.*)",      
        "dest": "/dist/$1"
      }
    ]
  }
  